pipeline {
    agent any
    stages {
        stage('number 1') {
            steps {
                echo 'Hello World'
            }
            }
        
        stage('number 2') {
            steps {
                sleep 30
            }
            }
        stage('number 3') {
            steps {
                echo 'Hello pune'
            }        
             
        }
    }
}
